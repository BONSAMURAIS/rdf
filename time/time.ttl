<http://rdf.bonsai.uno/time/>
  a dtype:Dataset ;
  dc:title "Time intervals"@en ;
  dc:description "Time intervals used in the BONSAI ontology"@en ;
  foaf:homepage <http://rdf.bonsai.uno/time/documentation.html> ;
  ns0:preferredNamespaceUri "http://rdf.bonsai.uno/time/#" ;
  owl:versionInfo "Version 0.1 - 2019-03-25"@en ;
  dc:modified "2019-03-25"^^xsd:date ;
  dc:publisher "bonsai.uno" ;
  dc:creator <http://bonsai.uno/foaf/bonsai.rdf#bonsai> ;
  dc:contributor "Matteo Lissandrini";
  cc:license <http://creativecommons.org/licenses/by/3.0/> ;
  rdfs:comment """We require time interval to register the temporal extent of activities, for instance.
               """@en .



<http://rdf.bonsai.uno/time/2011> a time:ProperInterval;

  time:hasBeginning [
      a time:Instant ;
      time:inXSDDate "2011-01-01"^^xsd:date ;
  ] ;

  time:hasDurationDescription [
       a time:DurationDescription;
       time:years "1"^^xsd:integer ;
  ];

  time:hasEnd [
      a time:Instant ;
      time:inXSDDate "2011-12-31"^^xsd:date ;
  ] ;

  owl:sameAs <http://reference.data.gov.uk/doc/year/2011> ;
  owl:sameAs <https://www.wikidata.org/wiki/Q1994> .


